    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src tests",
    "fix": "eslint src tests --fix",
    "jest": "jest --verbose --runInBand --passWithNoTests",
    "test:unit": "yarn jest",
    "test:integration": "yarn cypress run --env env=test --spec 'tests/integration/tests/**/*'",
    "test:mutation": "stryker run",
    "test:coverage": "jest --coverage tests src/components --coverageThreshold='{\"global\":{\"statements\":\"90\", \"functions\":\"90\", \"branches\":\"90\", \"lines\":\"90\"}}'"
  },
  "dependencies": {
    "elder-futhark": "^1.0.2",
    "futhorc": "^1.0.1",
    "futhork": "^1.1.1",
    "next": "^11.1.0",
    "next-pwa": "^5.3.1",
    "next-sitemap": "^1.6.140",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reset-css": "^5.0.1",
    "sass": "^1.38.2",
    "younger-futhark": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@stryker-mutator/core": "^5.2.2",
    "@stryker-mutator/jest-runner": "^5.2.2",
    "babel-jest": "^27.0.6",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
