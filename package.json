    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src tests",
    "fix": "eslint src tests --fix",
    "jest": "jest --verbose --runInBand --passWithNoTests",
    "test:unit": "yarn jest",
    "test:integration": "yarn cypress run --env env=test --spec 'tests/integration/tests/**/*'",
    "test:mutation": "stryker run",
    "test:coverage": "jest --coverage tests src/components --coverageThreshold='{\"global\":{\"statements\":\"90\", \"functions\":\"90\", \"branches\":\"90\", \"lines\":\"90\"}}'"
  },
  "dependencies": {
    "elder-futhark": "^1.0.3",
    "futhorc": "^1.0.2",
    "futhork": "^1.1.3",
    "next": "^12.0.9",
    "next-pwa": "^5.4.4",
    "next-sitemap": "^2.1.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reset-css": "^5.0.1",
    "sass": "^1.49.4",
    "younger-futhark": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@stryker-mutator/core": "^5.2.3",
    "@stryker-mutator/jest-runner": "^5.2.3",
    "babel-jest": "^27.4.6",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.0.9",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "react-test-renderer": "^17.0.2"
  }
